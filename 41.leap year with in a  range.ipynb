    "#leap year with in a  range\n",
    "start=int(input(\"enter starting year:\"))\n",
    "end=int(input(\"enter ending year:\"))\n",
    "years=[]\n",
    "for year in range(start,end+1):\n",
    "    if year%400==0:\n",
    "        years.append(year)\n",
    "    elif year%4==0 and year%100!=0:\n",
    "        years.append(year)\n",
    "print(years)"
