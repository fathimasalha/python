   "source": [
    "#dictionary sort in ascending and descending order.\n",
    "a={'tiya':12,'joel':13,'dev':15,'ben':14}\n",
    "l=list(a.items())\n",
    "l.sort()            \n",
    "print('Ascending order is:',l)\n",
    "l=list(a.items())\n",
    "l.sort(reverse=True)\n",
    "print('Descending order is:',l)\n",
    "dict=dict(l)\n",
    "print(\"Dictionary\",dict)"
