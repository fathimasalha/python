    "#check amstrong or not\n",
    "a=input(\"Enter a number: \")\n",
    "r=0\n",
    "for i in range(len(a)):\n",
    "    b=int(a[i])\n",
    "    r=r+(b** int(len(a)))\n",
    "if r==int(a):\n",
    "    print(a,\"is a amstrong number\")\n",
    "elif r!=int(a):\n",
    "    print(a,\"is not a amstrong number\")\n",
    "    \n"
