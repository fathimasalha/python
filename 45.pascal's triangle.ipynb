    "#Print Pascal's Triangle \n",
    "n=int(input(\"enter a range:\"))\n",
    "for i in range(1, n+1):\n",
    "    for j in range(0, n-i+1):\n",
    "        print(' ',end='')\n",
    "    C=1\n",
    "    for j in range(1,i+1):\n",
    "        print(' ',C,sep='',end='')\n",
    "        C=C*(i - j)//j\n",
    "    print()"
